table Feature {
  name:string (key);
  value:float;
  hash:uint64;
}

table Namespace {
  name:string;
  features:[Feature];
}

table Label {
  label:float;
  weight:float;
}

table Example {
  namespaces:[Namespace];
  label:Label;
}

table ExampleCollection {
  examples:[Example];
}

root_type ExampleCollection;